{
  "name": "order-commander",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "8.7.0",
    "npm": "5.4.2"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon .",
    "client": "npm start --prefix client",
